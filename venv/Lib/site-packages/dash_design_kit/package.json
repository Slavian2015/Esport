{
  "name": "dash_design_kit",
  "version": "1.3.0",
  "description": "Beautifully designed and thoroughly tested, Dash Design Kit is a premium set of building blocks, editable templates, and themes for professional Dash app development. No CSS required.",
  "main": "build/index.js",
  "scripts": {
    "start": "webpack-serve ./webpack.serve.config.js --open",
    "build:js-dev": "webpack --mode development",
    "build:js": "webpack --mode production",
    "build:py": "dash-generate-components ./src/lib/components dash_design_kit",
    "build:py-activated": "(. venv/bin/activate || venv\\scripts\\activate && npm run build:py)",
    "build:all": "npm run build:js && npm run build:js-dev && npm run build:py",
    "build:all-activated": "(. venv/bin/activate || venv\\scripts\\activate && npm run build:all)",
    "build:package": "npm run build:all && python build.py"
  },
  "author": "Chris P chris@plot.ly",
  "license": "Commercial",
  "dependencies": {
    "chroma-js": "^2.0.1",
    "fast-isnumeric": "^1.1.2",
    "file-saver": "^1.3.8",
    "fscreen": "^1.0.2",
    "ramda": "^0.26.1",
    "rc-slider": "^8.4.0",
    "react-beautiful-dnd": "^6.0.2",
    "react-color": "^2.14.1",
    "react-copy-to-clipboard": "^5.0.1",
    "react-popout": "^1.0.1",
    "react-select": "^1.2.1",
    "react-toggle": "^4.0.2",
    "tinycolor2": "^1.4.1",
    "url-loader": "^1.1.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "copyfiles": "^2.0.0",
    "css-loader": "^0.28.11",
    "dash-table": "^4.3.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.9.1",
    "exec-sh": "^0.3.2",
    "npm": "^6.1.0",
    "react": ">=0.14",
    "react-docgen": "^3.0.0",
    "react-dom": ">=0.14",
    "style-loader": "^0.21.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1",
    "webpack-serve": "^1.0.2"
  },
  "engines": {
    "node": ">=8.11.0",
    "npm": ">=6.1.0"
  }
}
